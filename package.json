{
  "name": "bind-rest",
  "version": "1.0.6",
  "description": "Framework for creating REST API with decorators and dependency injection",
  "private": false,
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
