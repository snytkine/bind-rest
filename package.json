    "inflate": "0.0.7",
    "inflation": "2.0.0",
    "is-stream": "^2.0.0",
    "jmespath": "^0.15.0",
    "jsonpath": "^1.0.2",
    "jsonschema": "1.1.1",
    "raw-body": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "require-from-string": "^2.0.2",
